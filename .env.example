# Copy to .env and fill in your FreeAgent OAuth credentials and tokens (if caching locally)
FREEAGENT_OAUTH_ID=your-oauth-identifier
FREEAGENT_OAUTH_SECRET=your-oauth-secret
FREEAGENT_OAUTH_REDIRECT_URI=http://127.0.0.1:8888
FREEAGENT_SCOPE=full

# Optional: seed tokens if you already have them; otherwise run the auth command
FREEAGENT_ACCESS_TOKEN=
FREEAGENT_REFRESH_TOKEN=
FREEAGENT_EXPIRES_AT=
